{
  "name": "tictactoejs",
  "version": "1.0.0",
  "description": "Tic Tac Toe Game",
  "main": "app.js",
  "scripts": {
    "start": "webpack --watch",
    "test": "mocha --require babel-register --require tests/setup  tests/unit --recursive",
    "local-test": "mocha --require babel-register --require tests/setup  tests/unit --recursive --watch",
    "e2e-test": "protractor ./tests/e2e-config.js",
    "e2e-setup": "webdriver-manager start",
    "e2e-update": "./node_modules/protractor/bin/webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rampage1213/TicTacToeJS.git"
  },
  "author": "Quintin R",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rampage1213/TicTacToeJS/issues"
  },
  "homepage": "https://github.com/rampage1213/TicTacToeJS#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.0",
    "protractor": "^4.0.2",
    "webpack": "^1.13.1"
  }
}
